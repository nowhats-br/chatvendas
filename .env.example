# Configurações do Supabase
VITE_SUPABASE_URL=https://your-project-id.supabase.co
VITE_SUPABASE_ANON_KEY=your-supabase-anon-key

# URLs dos Serviços WhatsApp
# Para desenvolvimento local
VITE_BAILEYS_URL=http://localhost:3001
VITE_WEBJS_URL=http://localhost:3002

# Para produção (substitua pelo IP/domínio do seu servidor)
# VITE_BAILEYS_URL=http://your-server-ip:3001
# VITE_WEBJS_URL=http://your-server-ip:3002

# Ambiente de execução
VITE_NODE_ENV=development

# Configurações dos Serviços (Produção)
# Portas dos serviços
BAILEYS_PORT=3001
WEBJS_PORT=3002

# IDs de sessão para produção
SESSION_ID=prod_session_unique_id

# URL do frontend para CORS (produção)
FRONTEND_URL=http://your-domain.com

# Host para bind dos serviços (produção)
HOST=0.0.0.0

# Configurações de logging
LOG_LEVEL=info

# Configurações de segurança
# Chave secreta para JWT (se implementado futuramente)
# JWT_SECRET=your-super-secret-jwt-key

# Configurações de banco de dados local (se necessário)
# DATABASE_URL=postgresql://user:password@localhost:5432/chatvendas

# Configurações de email (se implementado futuramente)
# SMTP_HOST=smtp.gmail.com
# SMTP_PORT=587
# SMTP_USER=your-email@gmail.com
# SMTP_PASS=your-app-password

# Configurações de armazenamento (se implementado futuramente)
# AWS_ACCESS_KEY_ID=your-aws-access-key
# AWS_SECRET_ACCESS_KEY=your-aws-secret-key
# AWS_REGION=us-east-1
# AWS_BUCKET_NAME=your-bucket-name

# Configurações de monitoramento (se implementado futuramente)
# SENTRY_DSN=https://your-sentry-dsn@sentry.io/project-id

# Configurações de cache Redis (se implementado futuramente)
# REDIS_URL=redis://localhost:6379